<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.jun.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginSuccessServlet</servlet-name>
    <servlet-class>com.jun.servlet.LoginSuccessServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Talk</servlet-name>
    <servlet-class>com.jun.servlet.Talk</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Messagebox</servlet-name>
    <servlet-class>com.jun.servlet.Messagebox</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginOut</servlet-name>
    <servlet-class>com.jun.servlet.LoginOut</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FirstFilter</servlet-name>
    <servlet-class>com.jun.servlet.FirstFilter</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SessionListener</servlet-name>
    <servlet-class>com.jun.servlet.SessionListener</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginSuccessServlet</servlet-name>
    <url-pattern>/servlet/LoginSuccessServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Talk</servlet-name>
    <url-pattern>/servlet/Talk</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Messagebox</servlet-name>
    <url-pattern>/servlet/Messagebox</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginOut</servlet-name>
    <url-pattern>/servlet/LoginOut</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FirstFilter</servlet-name>
    <url-pattern>/servlet/FirstFilter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SessionListener</servlet-name>
    <url-pattern>/servlet/SessionListener</url-pattern>
  </servlet-mapping>

</web-app>